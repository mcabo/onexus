<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.1.0">


    <!-- Resource activation -->
    <reference id="resourceRegister" interface="org.onexus.resource.api.IResourceRegister" />
    <bean id="collectionResourceActivator" class="org.onexus.resource.api.ResourceActivator" init-method="init">
        <property name="resourceRegister" ref="resourceRegister"/>
        <property name="resourceType" value="org.onexus.collection.api.Collection" />
    </bean>


</blueprint>